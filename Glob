##glob for multiple files reading 

import glob
import cv2

file_list = glob.glob('images/*.*')
print(file_list)

##################################
my_list = []

path = 'images/*.*'
for file in glob.glob(path):
    print(file)
    a = cv2.imread(file)
    my_list.append(a)
  
 
from matplotlib import pyplot as plt
plt.imshow(my_list[2])
#########################
